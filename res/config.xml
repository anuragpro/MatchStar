<?xml version='1.0' encoding='utf-8'?>
<widget id="MatchStars" version="0.0.1"
xmlns="http://www.w3.org/ns/widgets"
xmlns:cdv="http://cordova.apache.org/ns/1.0">
<name>MatchStar</name>
<description>Matching Stars for matrimony</description>
<author email="matchcharts@gmail.com" href="http://www.astraura.org">
Astaura
</author>
<content src="index.html" />
<!-- This now applies only to HTTP and HTTPS on Android -->
< access origin="*" browserOnly="true"/>
< gap:plugin name="org.apache.cordova.inappbrowser" />
<!-- These are needed in Cordova 3.6 and newer for Android -->
<access origin="http:*/*"/>
<access origin="https:*/*"/>
<access origin="mailto:*"/>
<access origin="tel:*"/>
 <plugin name="cordova-plugin-whitelist" version="1"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>

  <platform name="android">
  <allow-intent href="market:*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>    
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
    <feature name="InAppBrowser">
      <param name="ios-package" value="CDVInAppBrowser" />
  </feature>
  <feature name="InAppBrowser" />
<feature name="InAppBrowser">
      <param name="android-package" value="org.apache.cordova.InAppBrowser" />
  </feature>

</widget>
